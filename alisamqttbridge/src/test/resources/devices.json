{
	"devices": [
		{
            "id": "abc-123",
            "name": "лампa",
            "description": "цветная лампа",
            "room": "спальня",
            "type": "devices.types.light",
            "capabilities": [
                {
                    "type": "devices.capabilities.range",
                    "retrievable": true,
                    "parameters": {
                        "instance": "brightness",
                        "unit": "unit.percent",
                        "range": {
                            "min": 0,
                            "max": 100,
                            "precision": 10
                        }
                    }
                },
                {
                    "type": "devices.capabilities.on_off"
                },
                {
                    "type": "devices.capabilities.color_setting",
                    "parameters": {
                        "color_model": "hsv",
                        "temperature_k": {
                            "min": 2700,
                            "max": 9000,
                            "precision": 1
                        }
                    }
                }
            ],
            "properties": [
	            {
                    "type": "devices.properties.float",
                    "retrievable": true,
                    "parameters": {
                        "instance": "temperature",
                        "unit": "unit.temperature.celsius"
                    }
	            }
            ],
            "device_info": {
                "manufacturer": "Provider2",
                "model": "hue g11",
                "hw_version": "1.2",
                "sw_version": "5.4"
            },
            "custom_data": {
              "foo": 1,
              "bar": "two",
              "baz": false,
              "qux": [1, "two", false],
              "quux": {
                "quuz": {
                  "corge": []
                }
              }
            },
			
			"rules": [
				{
					"alisa": { "capability": "devices.capabilities.on_off", "instance": "on" },
					"mqtt": { "state": "house/outlet/bedroom/state", "commands": "house/outlet/bedroom/command" },
					"valueMapsToAlisa": [
						{ "type": "value", "from": "1", "to": "true" },
						{ "type": "value", "from": "0", "to": "false" }
					],
					"valueMapsToMqtt": [ 
						{ "type": "value", "from": "true", "to": "1" },
						{ "type": "value", "from": "false", "to": "0" }
					]
				},
				{
					"alisa": { "property": "devices.properties.float", "instance": "temperature" },
					"mqtt": { "state": "house/outlet/bedroom/statetemp" }
				}
			]
		}
	]
}